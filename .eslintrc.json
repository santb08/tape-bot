{
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  "plugins": ["prettier"],
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error"
  },
  "ignorePatterns": ["node_modules", "build"]
}
